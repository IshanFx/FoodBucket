<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>FoodServlet</servlet-name>
        <servlet-class>com.food.adminservlet.FoodServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name>
        <servlet-class>com.food.adminservlet.CustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>com.food.adminservlet.OrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FoodModifyServlet</servlet-name>
        <servlet-class>com.food.adminservlet.FoodModifyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FoodRemoveServlet</servlet-name>
        <servlet-class>com.food.adminservlet.FoodRemoveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportServlet</servlet-name>
        <servlet-class>com.food.adminservlet.ReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerMailController</servlet-name>
        <servlet-class>com.foodbucket.cusAdminModel.CustomerMailController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FoodServlet</servlet-name>
        <url-pattern>/backend/FoodServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name>
        <url-pattern>/backend/CustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/backend/OrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FoodModifyServlet</servlet-name>
        <url-pattern>/backend/FoodModifyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FoodRemoveServlet</servlet-name>
        <url-pattern>/backend/FoodRemoveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportServlet</servlet-name>
        <url-pattern>/ReportServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerMailController</servlet-name>
        <url-pattern>/backend/CustomerMailController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
